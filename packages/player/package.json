{
  "name": "atpls-player",
  "version": "3.5.1",
  "private": true,
  "scripts": {
    "setup": "echo 'Done.'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:types": "tsc --noUnusedLocals",
    "lint": "tslint --fix 'src/**/*.{ts,tsx}'",
    "build:analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject",
    "api:mock": "json-server -p 1234 --watch server/db.json"
  },
  "dependencies": {
    "@atpls/add-resize-observer": "^1.0.0",
    "@atpls/active-audio-context": "^1.0.0",
    "@atpls/audioset": "^1.0.0",
    "@atpls/test-audio-codecs": "^1.0.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@types/debug": "^4.1.5",
    "@types/jest": "^24.9.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-router-dom": "^5.1.5",
    "debug": "^4.1.1",
    "husky": "^3.1.0",
    "lint-staged": "^10.2.2",
    "lodash.debounce": "^4.0.8",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-css-collapse": "^4.0.6",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-responsive": "^8.0.3",
    "react-router-dom": "^5.1.0",
    "react-scripts": "^3.4.1",
    "resize-observer-polyfill": "^1.5.1",
    "screenfull": "^5.0.2",
    "standardized-audio-context": "^23.1.2",
    "typescript": "^3.8.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test:types"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-responsive": "^8.0.2",
    "prop-types": "^15.6.0",
    "json-server": "^0.16.1",
    "source-map-explorer": "^2.4.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0"
  }
}
