{
  "name": "atpls-player",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "serverless deploy -v && yarn run cloudfront:invalidate",
    "cloudfront:invalidate": "aws cloudfront create-invalidation --distribution-id E4L5YC3BGPSO1 --paths '/*'",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "chroma-js": "^2.0.3",
    "cxs": "^6.2.0",
    "d3": "^5.9.2",
    "d3-geo-projection": "^2.6.0",
    "react": "^16.8.6",
    "react-device-detect": "^1.6.2",
    "react-dom": "^16.8.6",
    "react-modal": "^3.8.1",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0",
    "screenfull": "^4.2.0",
    "tiny-emitter": "^2.1.0",
    "topojson": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "fullstack-serverless": "^0.5.8",
    "husky": "^2.2.0",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0"
  }
}
