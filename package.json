{
  "name": "atpls-player",
  "version": "3.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:types": "tsc --noUnusedLocals",
    "lint": "tslint --fix 'src/**/*.{ts,tsx}'",
    "deploy": "serverless deploy -v && yarn run cloudfront:invalidate",
    "build:analyze": "source-map-explorer 'build/static/js/*.js'",
    "cloudfront:invalidate": "aws cloudfront create-invalidation --distribution-id E4L5YC3BGPSO1 --paths '/*'",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@types/chroma-js": "^1.4.3",
    "@types/d3": "^5.7.2",
    "@types/debug": "^4.1.5",
    "@types/jest": "^24.0.23",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.2",
    "@types/topojson": "^3.2.2",
    "chroma-js": "^2.1.0",
    "d3": "^5.12.0",
    "d3-geo-projection": "^2.7.1",
    "debug": "^4.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "lodash.debounce": "^4.0.8",
    "prettier": "^1.19.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-responsive": "^8.0.1",
    "react-router-dom": "^5.1.0",
    "react-scripts": "3.2.0",
    "screenfull": "^5.0.0",
    "topojson": "^3.0.2",
    "typescript": "^3.7.2"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "singleQuote": false,
    "semi": true,
    "newline-before-return": true,
    "no-duplicate-variable": [
      true,
      "check-parameters"
    ],
    "no-var-keyword": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test:types"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-responsive": "^8.0.2",
    "serverless": "^1.57.0",
    "source-map-explorer": "^2.0.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.1.0"
  }
}
